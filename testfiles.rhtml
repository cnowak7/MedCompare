	<table bgcolor='white' width='90%'>
   		<tr><td><%= render "layouts/header" %></td></tr>
   		<tr><td><%= yield %></td></tr>
   		<tr><td><%= render "layouts/footer" %></td> </tr>
	</table>


	Navigation
	.nav {
	position: fixed;
	top: 11%;
	left: 5%;
	background-color: #333;
	width: 90%;
	
	}
	
.nav a:link {
	background-color: #333;
    color: white;
    padding: 14px 12px;
    text-align: center;	
    text-decoration: none;
    display: inline-block;
}

.nav a:visited {
	background-color: #333;
    color: white;
}

.nav a:hover {
	background-color: #111;
	}

.nav ul {
	list-style-type: none;
    margin: 0;
    overflow: hidden;
    background-color: #333;
	}
	
.nav li {
	display: block;
    color: white;
    text-align: center;
	}


li{
	text-decoration: none;
	list-style: none;
	font-size: 20px;
	float: left;
	
	}

Login/signup
.login
	{
	position: fixed;
	margin-left: 50%;
	top: 11%;
	
	background-color: #333;
    color: white;
    padding: 14px 0px;
    text-align: center;	
    text-decoration: none;
    display: inline-block;
	}
	
.sign_up
	{
	border-radius: 15px 50px;
	position: fixed;
	margin-left: 78%;
	top: 12%;
	color: black;
	
    background: #94B3FF  ;
	padding: 10px; 
    width: 150px;
    height: 30px; 
	float: center;
	
	}

